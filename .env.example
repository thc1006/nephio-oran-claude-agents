# Nephio O-RAN Environment Configuration for R5/L Release

# Go 1.24.6 FIPS 140-3 usage capability
# Enable FIPS mode for production environments (requires validated builds)
GODEBUG=fips140=on

# Nephio R5 Configuration
NEPHIO_VERSION=r5.0.1
NEPHIO_NAMESPACE=nephio-system

# O-RAN L Release Configuration
ORAN_RELEASE=l-release
ORAN_NAMESPACE=oran-system

# Kubernetes Configuration
KUBERNETES_VERSION=1.32.0
KUBECTL_VERSION=1.32.0

# ArgoCD Configuration (Primary GitOps in R5)
ARGOCD_VERSION=3.1.0
ARGOCD_NAMESPACE=argocd

# Container Configuration
DOCKER_VERSION=24.0.0
CONTAINERD_VERSION=1.7.0

# Python Configuration for O-RAN L Release O1 Simulator
PYTHON_VERSION=3.11.0
PYTHON_ENV=production

# Development Configuration
NODE_ENV=development
LOG_LEVEL=info
DEBUG=false

# Security Configuration
TLS_VERIFY=true
SECURE_HEADERS=true
CSP_ENABLED=true